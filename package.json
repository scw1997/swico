{
  "name": "swico",
  "version": "2.8.0-beta.10",
  "description": "Simple, efficient, and practical front-end framework",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "default": "./src/index.js",
      "types": "./src/index.d.ts"
    },
    "./react": {
      "default": "./src/index.react.js",
      "types": "./src/index.react.d.ts"
    },
    "./vue": {
      "default": "./src/index.vue.js",
      "types": "./src/index.vue.d.ts"
    }
  },
  "keywords": [
    "typescript",
    "javascript",
    "ecmascript",
    "react",
    "vue",
    "swico",
    "rspack"
  ],
  "bin": {
    "swico": "./src/bin.js"
  },
  "scripts": {
    "compile": "tsc && ts-node ./src/task.ts",
    "publish:offical": "npm run compile && cd ./dist && npm publish",
    "publish:beta": "npm run compile && cd ./dist && npm publish --tag=beta",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx} "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scw1997/swico.git"
  },
  "author": "scw",
  "license": "ISC",
  "dependencies": {
    "@rspack/dev-server": "1.1.4",
    "@vue/compiler-sfc": "3.5.23",
    "chalk": "4.1.2",
    "chokidar": "3.6.0",
    "commander": "12.0.0",
    "cross-spawn": "7.0.5",
    "css-loader": "7.1.2",
    "eslint-rspack-plugin": "4.2.1",
    "fs-extra": "11.3.0",
    "html-loader": "5.1.0",
    "html-webpack-plugin": "5.6.4",
    "less": "4.3.0",
    "less-loader": "12.2.0",
    "ora": "5.4.1",
    "portfinder": "1.0.32",
    "postcss": "8.5.3",
    "postcss-loader": "8.1.0",
    "qs": "6.13.0",
    "react-router": "7.5.0",
    "sass-embedded": "1.86.3",
    "sass-loader": "16.0.5",
    "style-loader": "3.3.0",
    "ts-checker-rspack-plugin": "1.1.1",
    "ts-node": "10.9.1",
    "vue-loader": "17.4.2",
    "vue-router": "4.5.0",
    "vue-style-loader": "4.1.3",
    "webpack-bundle-analyzer": "4.9.1",
    "webpack-merge": "6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@rspack/core": "1.6.1",
    "@types/node": "22.13.10",
    "@types/react": "~19.2.0",
    "@types/react-dom": "~19.2.0",
    "eslint": "9.24.0",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "react": "~19.2.0",
    "react-dom": "~19.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1",
    "vue": "~3.5.23"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
